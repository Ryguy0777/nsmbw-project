#pragma once

#include <cstddef>

// Placement new

constexpr inline void* operator new([[maybe_unused]] std::size_t size, void* ptr) noexcept
{
    return ptr;
}

constexpr inline void* operator new[]([[maybe_unused]] std::size_t size, void* ptr) noexcept
{
    return ptr;
}

namespace std
{
enum class align_val_t : std::size_t {
};
} // namespace std